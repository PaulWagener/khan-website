<div class="end-of-stack-info">
    <div class="title clearfix">
        <div class="stack-stats">
            <p data-desc="Your longest streak in this stack.">{{longestStreak}} Streak!</p>
            <p data-desc="Your total speedy answers for this stack.">{{speedyCards}} Speedy Answers</p>
            <p data-desc="Your total leaves for this stack.">{{totalLeaves}} Leaves</p>
        </div>
        <h1>TOTAL STACK DOMINATION</h1>
    </div>
    <div class="progress-graph">
	    {{#each progress}}
	    	<h2 class="skill-bar-title">{{displayName}}</h2>
	    	<div class="skill-bar orange">
	    		<div class="fill red" style="width:{{end}}%;">
	    			<div class="description">
		    			<!-- {{end}} (change: {{change}}) -->
		    		</div>
	    		</div>
	    		<div class="fill orange" style="width:{{start}}%;">
	    			<div class="description">
		    			<!-- {{end}} (change: {{change}}) -->
		    		</div>
	    		</div>
	        </div>
	    {{/each}}
    </div>
    <div class="stack-controls">
      <!-- TODO Jason/Kamens: Get real URLs in here -->
      <a href="/exercisedashboard" class="simple-button action-gradient left">Take a Break</a><a href="/math/arithmetic/addition-subtraction/e/" class="simple-button action-gradient green right">More Stacks!</a>
    </div>
</div>
