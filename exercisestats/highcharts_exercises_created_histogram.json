{
    "chart": {
        "renderTo": "container",
        "plotBackgroundColor": "rgba(35,37,38,0)",
        "backgroundColor": "rgba(35,37,38,100)",
        "borderColor": "rgba(35,37,38,100)",
        "lineColor": "rgba(35,37,38,100)",
        "plotBorderColor": "rgba(35,37,38,100)",
        "plotBorderWidth": null,
        "plotShadow": false,
        "height": 340,
    },
    "colors": [ "#058DC7", "#50B432", "#EF561A" ],
    "credits": { "enabled": false },
    "title": { "text": "Exercise creation over time" },
    "legend": { "enabled": false },
    "plotOptions": {
        "column": {
            "pointPadding": 0,
            "groupPadding": 0
        }
    },
    "series": [
        {% for data in series %}
            {
                "type": "{{ data.type }}",
                "data": {{ data.values }},
                "yAxis": {{ data.axis }},
            }{% if not forloop.last %},{% endif %}
        {% endfor %}
    ],
    "yAxis": [
        {% for axis in axes %}
            {
                "title": { "text": null },
                "allowDecimals": false,
                "max": {{ axis.max }}
                {% if forloop.last %}
                    , "opposite": true
                {% endif %}
            }{% if not forloop.last %},{% endif %}
        {% endfor %}
    ],
    "xAxis": { "type": "datetime" }
}
